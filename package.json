{
  "name": "yt_mp3_downloader",
  "version": "1.0.1",
  "description": "A node.js script that downloads songs from Youtube",
  "main": "index.js",
  "preferGlobal": true,
  "scripts": {
    "start": "node index.js",
    "build": "pkg . -o ./build/yt_mp3.exe --public"
  },
  "bin": {
    "yt_mp3": "index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "commander": "^2.18.0",
    "ffmpeg-binaries": "^3.2.2-3",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
    "node-id3": "^0.1.5",
    "ytdl-core": "^0.20.4",
    "yt-playlists": "file:./yt-playlists"
  },
  "keywords": [
    "youtube",
    "mp3",
    "youtube to mp3",
    "youtube playlists",
    "youtube songs"
  ],
  "homepage": "https://github.com/LevitatingBusinessMan/yt_mp3#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/LevitatingBusinessMan/yt_mp3.git"
  },
  "author": "LevitatingBusinessMan",
  "license": "MIT"
}
